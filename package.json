{
  "name": "hln-a",
  "version": "2.0.2",
  "description": "HLN-A is a Discord companion app designed to assist a survivor in their day to day ARK operations",
  "keywords": [
    "hlna",
    "hln-a",
    "hln-a-discord-bot",
    "hlna-discord-bot",
    "ark-survival-discord-bot",
    "ark discord bot",
    "typescript-discord-bot"
  ],
  "main": "dist/client/bot.js",
  "scripts": {
    "server": "pm2 start dist/server/server.js --name \"hlna-api\"",
    "client": "pm2 start dist/client/index.js --name \"HLN-A\" --watch",
    "build": "concurrently \"npm run server\" \"npm run client\"",
    "dev-server": "node dist/server/server.js",
    "dev": "concurrently \"nodemon dist/server/server.js\" \"nodemon dist/client/index.js\"",
    "deployslash": "node dist/deploy-commands.js",
    "deleteslash": "node dist/delete-commands.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ALCHElVlY/hlna.git"
  },
  "author": {
    "name": "William Holt",
    "discord": "Alchemy#1990"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ALCHElVlY/hlna/issues"
  },
  "homepage": "https://github.com/ALCHElVlY/hlna#readme",
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "discord.js": "^13.8.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.4.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^6.0.13",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.42",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15"
  }
}
